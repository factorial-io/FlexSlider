  "bugs": {
    "url": "https://github.com/woothemes/FlexSlider/issues"
  },
  "homepage": "https://github.com/woothemes/FlexSlider#readme",
  "dependencies": {
    "jquery": "^1.11.3"
  },
  "files": [
    "index.css",
    "index.js",
    "lib"
  ],
  "devDependencies": {
    "suitcss-components-test": "*",
    "suitcss-preprocessor": "~0.4.0"
  }
